		<!-- AI Accepts Deal -->
		<Row	Tag="LOC_DIPLO_ACCEPT_MAKE_DEAL_FROM_AI_LEADER_STR_ARAGON_ALFONSO_V_I_THE_MAGNANIMOUS_DE_TRASTAMARA_ANY"
				Language="en_US">
			<Text>The merchants shall enjoy the security established by the Book of the Consulate of the Sea.</Text>
		</Row>
