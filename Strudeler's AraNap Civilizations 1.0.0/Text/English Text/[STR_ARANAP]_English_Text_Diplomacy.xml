		<Row	Tag="LOC_DIPLO_REJECT_MAKE_DEAL_FROM_AI_LEADER_STR_ARAGON_ALFONSO_V_I_THE_MAGNANIMOUS_DE_TRASTAMARA_ANY"
				Language="en_US">
			<Text>It cannot be done.</Text>
		</Row>
